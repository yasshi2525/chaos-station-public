{
  "name": "chaos-station",
  "private": true,
  "version": "0.1.0",
  "description": "",
  "scripts": {
    "postinstall": "npm run build",
    "build": "rimraf ./script && tsc && akashic scan asset script",
    "watch": "tsc -w",
    "update": "akashic scan asset && akashic scan globalScripts",
    "lint": "eslint -c .eslintrc.js \"**/*.{ts,js}\" --fix",
    "start": "akashic-sandbox .",
    "start-serve": "akashic serve -s nicolive -w",
    "export-zip": "rimraf game.zip && akashic export zip -o game.zip -s",
    "export-html": "rimraf game.zip && akashic export html -o game.zip --atsumaru",
    "test": "jest --coverage --silent --testLocationInResults --ci --json --outputFile=\"report.json\"",
    "test:local": "npm run build && jest"
  },
  "author": "",
  "license": "",
  "devDependencies": {
    "@akashic/akashic-cli": "^2.16.17",
    "@akashic/akashic-engine": "^3.11.0",
    "@akashic/akashic-sandbox": "^0.22.6",
    "@akashic/eslint-config": "^1.1.0",
    "@akashic/headless-akashic": "^4.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@yasshi2525/pixi-image-packer": "^0.3.4",
    "eslint": "^8.36.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "pixi-filters": "^5.2.1",
    "pixi.js": "^7.2.3",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.0.5",
    "typescript": "^5.0.2"
  }
}
